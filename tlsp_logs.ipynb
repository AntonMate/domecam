    "from datetime import datetime, timedelta\n",
    "import os\n",
    "import sys\n",
    "\n",
    "dir_ = 'D:/astro/domecam/tlsp'\n",
    "file_corr = 'DC220322024901_2km.fits'\n",
    "catalog = 'D:/astro/domecam/mass_star.lst'\n",
    "delta_seconds = 150 # разница между временем в названии серии и в файле логов, секунды\n",
    "\n",
    "full_data_ = datetime.strptime('20' + file_corr[2:4] + '-' + file_corr[4:6] + '-' + file_corr[6:8] + ' ' + file_corr[8:10] + ':' + file_corr[10:12] + ':' + file_corr[12:14], '%Y-%m-%d %H:%M:%S')\n",
    "full_data_ = full_data_ + timedelta(hours=3) # перевод в местное время\n",
    "\n",
    "time_ = full_data_.time()\n",
    "time_ = timedelta(hours=time_.hour, minutes=time_.minute, seconds=time_.second)\n",
    "\n",
    "date_ = str(full_data_.date())\n",
    "\n",
    "print(f'{file_corr} = {full_data_} по местному времени')\n",
    "\n",
    "all_files_ = []\n",
    "for log_ in os.listdir(dir_):\n",
    "    if date_ in log_:\n",
    "        all_files_.append(log_)\n",
    "\n",
    "print(all_files_)\n",
    "found_somewhere = 0\n",
    "for file_ in all_files_:\n",
    "    line_lb = []\n",
    "    with open(f'{dir_}/{file_}') as f:\n",
    "        for line in f:\n",
    "            global_delta_time = 3000000\n",
    "            if '[RUN] -> INIT' in line:\n",
    "                time_found = datetime.strptime(line[19:27], '%H:%M:%S').time()\n",
    "                time_found = timedelta(hours=time_found.hour, minutes=time_found.minute, seconds=time_found.second)\n",
    "                if (time_ - time_found).seconds < global_delta_time and time_ > time_found:\n",
    "                    global_delta_time = (time_ - time_found).seconds\n",
    "                    line_lb.append(line)\n",
    "            \n",
    "    line_lb = line_lb[-1]\n",
    "\n",
    "    line_ub = []\n",
    "    with open(f'{dir_}/{file_}') as f:\n",
    "        for line in f:\n",
    "            metka = True\n",
    "            if '[RUN] -> INIT' in line:\n",
    "                time_found = datetime.strptime(line[19:27], '%H:%M:%S').time()\n",
    "                time_found = timedelta(hours=time_found.hour, minutes=time_found.minute, seconds=time_found.second)\n",
    "                metka = time_ > time_found\n",
    "                if metka == False:\n",
    "                    line_ub.append(line)\n",
    "\n",
    "    if not line_ub:\n",
    "        text=[]\n",
    "        with open(f'{dir_}/{file_}') as f:\n",
    "            copy = False\n",
    "            for line in f:\n",
    "                if line_lb in line:\n",
    "                    copy = True\n",
    "                if copy:\n",
    "                    text.append(line)\n",
    "        ra, dec = None, None\n",
    "        for item in text:\n",
    "            if 'createEQU RA0' in item:\n",
    "                print(item.strip())\n",
    "                ra = float(item.split()[-1])\n",
    "            if 'createEQU DEC0' in item:\n",
    "                print(item.strip())\n",
    "                dec = float(item.split()[-1])\n",
    "    else:\n",
    "        line_ub = line_ub[0]\n",
    "        print(line_lb.strip())\n",
    "\n",
    "        text=[]\n",
    "        with open(f'{dir_}/{file_}') as f:\n",
    "            copy = False\n",
    "            for line in f:\n",
    "                if line_lb in line:\n",
    "                    copy = True\n",
    "                if copy:\n",
    "                    text.append(line)\n",
    "        text = text[:text.index(line_ub)]\n",
    "        ra, dec = None, None\n",
    "        for item in text:\n",
    "            if 'createEQU RA0' in item:\n",
    "                print(item.strip())\n",
    "                ra = float(item.split()[-1])\n",
    "            if 'createEQU DEC0' in item:\n",
    "                print(item.strip())\n",
    "                dec = float(item.split()[-1])\n",
    "\n",
    "        print(line_ub.strip())\n",
    "\n",
    "    if ra is not None and dec is not None:\n",
    "        aim = timedelta(seconds=3600 * ra / 15)\n",
    "        with open(catalog) as f:\n",
    "            for line in f:\n",
    "                line = line.split()\n",
    "                line_ra = (':').join(line[2:5])\n",
    "                line_ra =  datetime.strptime(line_ra, '%H:%M:%S').time()\n",
    "                line_ra = timedelta(hours=line_ra.hour, minutes=line_ra.minute, seconds=line_ra.second)\n",
    "                if (abs(aim - line_ra)).seconds < 2:\n",
    "                    print(line[1], line[10], line[11])\n",
    "                    found_somewhere += 1\n",
    "\n",
    "                    print()\n",
    "\n",
    "\n",
    "    if ra is None and dec is None or found_somewhere == 0:\n",
    "        print('Тестовый файл или ошибка')\n",
    "        print()"
   ]
